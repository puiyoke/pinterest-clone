<br>
<div class="container">
  <div class="card-columns">
    <%@pin.each do |x|%>
      <div class="card">
      <a href="/pins/<%=x.id%>"><%= image_tag(x.image_url(:display), :class => "card-img") %></a>
      </div>
    <%end%>
  </div>
</div>


<script>
$( function() {
  var availableTags = [
    "<%@tag.each do |x|%>",
    "<%=x.name%>",
    "<%end%>",
  ];
  $( "#alltags" ).autocomplete({
    source: availableTags
  });
} );
</script>